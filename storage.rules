rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Allow users to read, write, and delete their own files
    match /images/{userId}/{allPaths=**} {
      allow read, write, delete: if request.auth != null && request.auth.uid == userId;
    }
    match /audio/{userId}/{allPaths=**} {
      allow read, write, delete: if request.auth != null && request.auth.uid == userId;
    }
  }
}
